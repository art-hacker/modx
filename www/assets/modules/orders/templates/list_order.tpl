<tr data-status="[+status+]" data-id="[+id+]">
  <td class='id'>
      <a href=''>[+id+]</a><br/>
      <small title='[+date+], в [+time+]'>[+date_diff+]</small>
  </td>
  <td>
      <b>[+fullname+]</b>, <a href='tel:[+phone+]'>[+phone+]</a><br/>
      <i>[+note+]</i>
  </td>
  <td>
      <a target='_blank' href='[+ymap+]'>[+city+]</a>
  </td>
  <td class='dates'>
      <span>[+date+]<br/>[+sentdate+]</span>
  </td>
  <td>
      <table class='items'>[+items+]</table>
  </td>
  <td class='price' data-costs="[+costs+]" data-profit="[+profit+]"><big><b>[+price+]</b> </big>руб.<br/><small><b>Федотов А. И.</b></small></td>
  <td class='info'>
      <i class='icon [+delivery+]' data-pickup='[+pickup+]'></i>
      <i class='icon [+payment+]'></i>
  </td>
  <td class='orderCheckbox'>
      <input type="checkbox" id="order_[+id+]" name="orderId[]" value='[+id+]'/>
      <label for="order_[+id+]"></label>
  </td>
</tr>
